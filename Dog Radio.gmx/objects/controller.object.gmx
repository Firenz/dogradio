        x = 360;
        y = 300;
        score = 0;
        global.intro = false;
        global.winner = false;
        //x_relative_for_mouse = 0;
        audio_pause_all();
        room_restart();
        room_goto(GameOverWinner);
    }


}else if(room == MainMenu){
    if( mouse_check_button(mb_left) &amp;&amp; mouse_x &gt; 280 &amp;&amp; mouse_x &lt; 520 &amp;&amp; mouse_y &gt; 300 &amp;&amp; mouse_y &lt; 345){
