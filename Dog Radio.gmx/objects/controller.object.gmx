        room_goto(HowTo);
    }
    
    if( mouse_check_button(mb_left) &amp;&amp; mouse_x &gt; 280 &amp;&amp; mouse_x &lt; 520 &amp;&amp; mouse_y &gt; 405 &amp;&amp; mouse_y &lt; 445){
        //room_restart();
        room_goto(Credits);
    }

    if( mouse_check_button(mb_left) &amp;&amp; mouse_x &gt; 280 &amp;&amp; mouse_x &lt; 520 &amp;&amp; mouse_y &gt; 455 &amp;&amp; mouse_y &lt; 495){
        //room_restart();
        game_end();
    }       
}else if(room == HowTo || room == Credits ){
