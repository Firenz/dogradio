    if( mouse_check_button(mb_left) &amp;&amp; window_mouse_get_x() &gt; 615 &amp;&amp; window_mouse_get_x() &lt; 770 &amp;&amp; mouse_y &gt; 520 &amp;&amp; mouse_y &lt; 565){
        x = 360;
        y = 300;
