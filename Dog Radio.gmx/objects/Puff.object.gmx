        <arguments>
          <argument>
            <kind>1</kind>
            <string>image_speed=0.3

if( x &gt;= 12192){
    x += 10;
}else{
    x +=global.dog_speed;
}

// Jumping
if(jumping){
    if(ascending &amp;&amp; y&gt;390){
        if(image_index &gt;= 3){
            image_index = 3;
        }
        y -=5;
    }
    else if(descending &amp;&amp; y&lt;480){
        if(image_index == 5){
            image_index = 5;
        }
        y +=5;
    }

    if(ascending &amp;&amp; y==390){
        if( x == (save_x + 175)){
            ascending = false;
            descending = true;
        }
            image_index = 3;
    }

    if(descending &amp;&amp; y==480){
        descending=false;
        jumping=false;
        sprite_index = Dog;
    }
}


//Barking
if ( !audio_is_playing(Bark) &amp;&amp; barking == true &amp;&amp; !jumping){
    barking = false;
}

// Crawling
if(crawling){
    if(time_to_stop_crawling == 0){
        sprite_index = Dog;
        crawling = false;
    }else{
        time_to_stop_crawling--;
    }
}

//Puff is hitted?
if(global.is_hitted){
    image_alpha = 0;
    alarm[1] = 10;
    global.is_hitted = false;
    global.invincible = true;
}

//Puff reached car?
if ( x &gt;= ( room_width - 210)){
    controller.x = 360;
    controller.y = 300;
    score = 0;
    global.intro = false;
    global.winner = true;
    audio_pause_all();
    room_restart();
    room_goto(GameOverWinner);
}



