        <arguments>
          <argument>
            <kind>1</kind>
            <string>

image_speed=0.3
//x ++;
x +=global.dog_speed;

//Cronometro

miliseg+=1
if miliseg&gt;room_speed
{seg+=1 miliseg=0}
if seg&gt;59
{minute+=1 seg=0}


if !(place_meeting(x,y,Obstacle)){
  hit = false;
}

//Jumping

if (keyboard_check(vk_up) &amp;&amp; !jumping){
    jumping = true;
    ascending = true;
    dist_jumping = x;
} 

if(jumping){
    //if(ascending &amp;&amp; y&gt;240){
    if(ascending &amp;&amp; y&gt;390){
        //y--;
        if(image_index &gt;= 3){
            image_index = 3;
        }
        y -=5;
    }
    //else if(descending &amp;&amp; y&lt;320){
    else if(descending &amp;&amp; y&lt;480){
        if(image_index == 3){
            image_index = 3;
        }
        //y++;
        y +=5;
    }

    //if(ascending &amp;&amp; y==240){
    if(ascending &amp;&amp; y==390){
        if( x == (save_x + 175)){
            ascending = false;
            descending = true;
        }
        if(image_index == 3){
            image_index = 3;
        }
    }

    //if(descending &amp;&amp; y==320){
    if(descending &amp;&amp; y==480){
        descending=false;
        jumping=false;
        if(image_index &lt; 0){
            image_index -=1;
        }
        else{
            sprite_index = Dog;
        }
        dist_jumping = x - dist_jumping;
    }
}


//Barking

if ( !audio_is_playing(Bark) &amp;&amp; barking == true &amp;&amp; !jumping){
    barking = false;
}

   
//Puff reached car?
if ( x &gt;= ( room_width - 210)){
    controller.x = 360;
    controller.y = 300;
    score = 0;
    global.intro = false;
    global.winner = true;
    audio_pause_all();
    room_restart();
    room_goto(GameOverWinner);
}



</string>
          </argument>
        </arguments>
      </action>
